      "source": [
        "import numpy as np\n",
        "\n",
        "# 行列 A と B を定義\n",
        "A = np.array([[0, 1], [2, 3], [4, 5]])\n",
        "B = np.array([[0, 1, 2, 3], [4, 5, 6, 7]])\n",
        "\n",
        "# 行列の積を計算\n",
        "C = np.dot(A, B)\n",
        "\n",
        "# 結果の行列を表示\n",
        "print(\"行列の積 C:\\n\", C)\n",
        "\n",
        "# 行列の最大要素を求める\n",
        "max_element = np.max(C)\n",
        "print(\"行列 C の要素の最大値:\", max_element)"
      ]
    }
  ]
